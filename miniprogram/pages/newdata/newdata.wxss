/* pages/newdata/newdata.wxss */

.cu-form-group .title {
  min-width: calc(4em + 30rpx);
}

.image_content{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 20px
}

.btn{
  width:90%;
  display: block;
  position: relative;
  top: 3rpx;
  font-weight: bold;
  border-radius: 47rpx;
  color:#ffffff;
  background: linear-gradient(to right, #accbebc2, #7ab9f8);
}

.btn-hover{
  position: relative;
  top: 3rpx;
  box-shadow:0px 0px 8px #999 inset; 
}

.v{
  margin: 35px;
  color: #ffffff;
}